// Control Flow Graph
digraph {
	1 [label="1: enter: abs_value(x)" peripheries=2 shape=oval]
	2 [label="1: exit: abs_value(x)" peripheries=2 shape=oval]
	6 -> 2 [color=green]
	3 [label="2: if: x < 0" peripheries=1 shape=diamond]
	1 -> 3 [color=red]
	4 [label="3: v: float = -x" peripheries=1 shape=oval]
	3 -> 4 [color=red]
	5 [label="5: v: float = x" peripheries=1 shape=oval]
	3 -> 5 [color=red]
	6 [label="6: return v" peripheries=1 shape=rectangle]
	4 -> 6 [color=red]
	5 -> 6 [color=green]
}
